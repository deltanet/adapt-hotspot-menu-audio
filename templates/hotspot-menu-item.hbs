<button role="button" class="base menu-item-hotspot {{#if _hotspotMenuItem._buttonIcon._isEnabled}}icon {{_hotspotMenuItem._buttonIcon._icon}}{{/if}} {{#if _isVisited}}visited{{/if}} {{#if _isComplete}}complete{{/if}}" style="top:{{_hotspotMenuItem.position._top}}%; left:{{_hotspotMenuItem.position._left}}%" data-id="{{_id}}">{{_hotspotMenuItem.title}}</button>
<div class="menu-item-inner{{#if _locked}} disabled{{/if}}">
	<button class="base menu-item-done icon icon-cross"></button>
	<div class="menu-item-title">
		<h5 class="menu-item-title-inner">{{{displayTitle}}}</h5>
	</div>
	<div class="menu-item-body">
		<div class="menu-item-body-inner">{{{body}}}</div>
	</div>
	<div class="menu-item-button">
        <button role="button" aria-label="{{linkText}} {{#if _isVisited}}{{_globals._accessibility._ariaLabels.visited}}{{/if}}" class="{{#if _isVisited}}visited{{/if}} accessible-text-block">
            {{#if _isVisited}}
                {{{linkVisitedText}}}
                    {{else}}
                {{{linkText}}}
            {{/if}}
        </button>
        {{#if duration}}
            <span class="menu-item-duration accessible-text-block" role="region" tabindex="0">{{{durationLabel}}} {{{duration}}}</span>
        {{/if}}
    </div>
</div>