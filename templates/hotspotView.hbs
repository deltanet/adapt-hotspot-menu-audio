{{#if _hotspotMenuAudio._audio._isEnabled}}
<div class="audio-controls">
	<div class="audio-inner">
		<button class="audio-toggle" id="{{_id}}" role="button"></button>
	</div>
</div>
{{/if}}
<div class="menu-container">
	<div class='menu-container-inner box-menu-inner clearfix'>
		{{#if displayTitle}}
		<div class="menu-title">
			<h1 class="menu-title-inner">
				{{{displayTitle}}}
			</h1>
		</div>
		{{/if}}
		{{#if body}}
		<div class="menu-body">
			<div class="menu-body-inner">
				{{{body}}}
			</div>
		</div>
		{{/if}}
		<div class="hotspot-container">
			<div class="hotspot-container-inner">
				<img class="hotspot-container-image" src="{{_graphic.src}}" alt="{{_graphic.alt}}" />
			<div class="hotspot-items clearfix"></div>
			</div>
		</div>
	</div>
</div>
